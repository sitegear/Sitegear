<?php
/**
 * @var \Sitegear\Core\View\View $view
 */
?>
<div class="sitegear-login-selector">
<?php
$output = array();
foreach ($view['links'] as $link) {
	if ($link['current']) {
		$output[] = $link['label'];
	} else {
		$output[] = sprintf('<a href="%s">%s</a>', $link['url'], $link['label']);
	}
}
echo implode(' | ', $output);
?>
</div>
